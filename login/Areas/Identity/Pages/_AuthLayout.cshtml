@{
    Layout = "/Views/Shared/_LoginAdmin.cshtml";
}



                        @RenderBody()

@section Scripts{
    @RenderSection("Scripts",required:false)
    <script>
        $(function(){
            var current = location.pathname;
            $('.nav-tabs li a').each(function(){
                var $this = $(this);
                if(current.indexOf($this.attr('href')) !== -1){
                    $this.addClass('active');
                }
            })
        })
    </script>
}
